<!-- Modal -->

<div class="container">
  <% if (maquinado) { %>
   
 
  <form action="/api/maquinado/<%= maquinado.idMaquinado %> " method="POST">
    <input id="idMaquinado" name="idMaquinado" value="<%= maquinado.idMaquinado %> " hidden>
    <div class="container-fluid">
        <div class="row text-center">
          <div class="col-4">
            <label for="fecha_solicitud" class="form-label"><b>Nombre Proyecto</b></label>
            <input type="text" class="form-control" id="fecha_solicitud" name="fecha_solicitud" value="<%= maquinado.nombre_ProyectoMA %> " disabled>
            </div>
          <div class="col-4">
            <label for="fecha_Solicitud" class="form-label"><b>Fecha Solicitud</b></label>
            <input type="date" class="form-control" id="fecha_Solicitud" name="fecha_Solicitud" value="<%= maquinado.fecha_Solicitud %>" >
            </div>
            <div class="col-4">
              <label for="fecha_Promesa" class="form-label"><b>Fecha Promesa</b></label>
              <input type="date" class="form-control" id="fecha_Promesa" name="fecha_Promesa" value="<%= maquinado.fecha_Promesa %>">
            </div>
           
        </div>
          <div class="row text-center my-2">
            <div class="col-4">
              <label for="fecha_Entrega" class="form-label"><b>Fecha Entrega</b></label>
              <input type="date" class="form-control" id="fecha_Entrega" name="fecha_Entrega" value="<%= maquinado.fecha_Entrega %>">
            </div>
            <div class="col-4">
              <label for="fecha_Prorroga" class="form-label"><b>Fecha Pr&oacute;rroga</b></label>
              <input type="date" class="form-control" id="fecha_Prorroga" name="fecha_Prorroga" value="<%= maquinado.fecha_Prorroga %>">
            </div>
            <div class="col-4">
              <label for="ubicacion" class="form-label"><b>Ubicaci&oacute;n</b></label>
              <input type="text" class="form-control" id="ubicacion" name="ubicacion" value="<%= maquinado.ubicacion %> ">
            </div>
          </div>
          <div class="row">
            
            <div class="col-4">
              <label for="quien_Realizo" class="form-label"><b>Quien Realizo</b></label>
              <input type="text" class="form-control" id="quien_Realizo" name="quien_Realizo" value="<%= maquinado.quien_Realizo %> ">
            </div>
            <div class="col-4">
              <label for="que_Realizo" class="form-label"><b>Que Realizo</b></label>
              <input type="text" class="form-control" id="que_Realizo" name="que_Realizo" value="<%= maquinado.que_Realizo %> ">
            </div>
          </div>
          <div class="row text-center my-2">
            <div class="col-12">
              <label for="observaciones" class="form-label"><b>observaciones</b></label>
              <textarea class="form-control" id="observaciones" name="observaciones" rows="3" ><%= maquinado.observaciones %> </textarea>
              </div>
          </div>
    </div>
    <br>
    <div class="text-center">
      <button type="submit" class="btn btn-primary"><i class='bx bx-save'></i> Guardar</button>
    </div>
  </div>
  </form>
  <% } %>
</div>

<%- include("templates/footer") %> 